# CompilerName=mingw64
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/win64
matlabdir = C:/Program Files/MATLAB/R2024a
startdir = D:/Research/Soft Robotics Toolbox/SoRoSim GitHub/SoRoSim/Differentiable_SoRoSim/Basic_functions

# Toolchain information
cc = C:\ProgramData\MATLAB\SupportPackages\R2024a\3P.instrset\mingw_w64.instrset\bin\gcc
cxx = C:\ProgramData\MATLAB\SupportPackages\R2024a\3P.instrset\mingw_w64.instrset\bin\g++
linker = C:\ProgramData\MATLAB\SupportPackages\R2024a\3P.instrset\mingw_w64.instrset\bin\gcc
linkerxx = C:\ProgramData\MATLAB\SupportPackages\R2024a\3P.instrset\mingw_w64.instrset\bin\g++

cflags =  -c -fexceptions -fno-omit-frame-pointer -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c99 -I "D:\Research\Soft Robotics Toolbox\SoRoSim GitHub\SoRoSim\Differentiable_SoRoSim\Basic_functions\codegen\mex\RigidJointDifferentialKinematics" -I "D:\Research\Soft Robotics Toolbox\SoRoSim GitHub\SoRoSim\Differentiable_SoRoSim\Basic_functions" -I "D:\Research\Soft Robotics Toolbox\SoRoSim GitHub\SoRoSim\Differentiable_SoRoSim\Basic_functions\codegen\mex\RigidJointDifferentialKinematics\interface" -I "C:\Program Files\MATLAB\R2024a\extern\include" -I "." -DMODEL=RigidJointDifferentialKinematics_mex -D__USE_MINGW_ANSI_STDIO=1 -msse2 -fno-predictive-commoning 

cxxflags =  -c -fexceptions -fno-omit-frame-pointer -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c++11 -I "D:\Research\Soft Robotics Toolbox\SoRoSim GitHub\SoRoSim\Differentiable_SoRoSim\Basic_functions\codegen\mex\RigidJointDifferentialKinematics" -I "D:\Research\Soft Robotics Toolbox\SoRoSim GitHub\SoRoSim\Differentiable_SoRoSim\Basic_functions" -I "D:\Research\Soft Robotics Toolbox\SoRoSim GitHub\SoRoSim\Differentiable_SoRoSim\Basic_functions\codegen\mex\RigidJointDifferentialKinematics\interface" -I "C:\Program Files\MATLAB\R2024a\extern\include" -I "." -DMODEL=RigidJointDifferentialKinematics_mex -D__USE_MINGW_ANSI_STDIO=1 -msse2 -fno-predictive-commoning 

ldflags =  -m64 -Wl,--no-undefined -shared -static -L"C:\Program Files\MATLAB\R2024a\extern\lib\win64\mingw64" -llibmx -llibmex -llibmat -lm -llibmwlapack -llibmwblas -o "RigidJointDifferentialKinematics_mex.mexw64" -L"$matlabdir\extern\lib\win64\mingw64" -llibmwblas -llibemlrt -llibcovrt -llibut -llibmwmathutil  RigidJointDifferentialKinematics_mex.def

linkobjs = 

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker @$out.rsp $linkobjs $ldflags
    rspfile = $out.rsp
    rspfile_content = $in

rule linkxx
    command = $linkerxx $in $linkobjs $ldflags

# Build
build $builddir/RigidJointDifferentialKinematics_data.o : cc $root/RigidJointDifferentialKinematics_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/RigidJointDifferentialKinematics_initialize.o : cc $root/RigidJointDifferentialKinematics_initialize.c
build $builddir/RigidJointDifferentialKinematics_terminate.o : cc $root/RigidJointDifferentialKinematics_terminate.c
build $builddir/RigidJointDifferentialKinematics.o : cc $root/RigidJointDifferentialKinematics.c
build $builddir/eml_mtimes_helper.o : cc $root/eml_mtimes_helper.c
build $builddir/mtimes.o : cc $root/mtimes.c
build $builddir/norm.o : cc $root/norm.c
build $builddir/eye.o : cc $root/eye.c
build $builddir/dinamico_adj.o : cc $root/dinamico_adj.c
build $builddir/_coder_RigidJointDifferentialKinematics_api.o : cc $root/interface/_coder_RigidJointDifferentialKinematics_api.c
build $builddir/_coder_RigidJointDifferentialKinematics_mex.o : cc $root/interface/_coder_RigidJointDifferentialKinematics_mex.c
build $builddir/_coder_RigidJointDifferentialKinematics_info.o : cc $root/interface/_coder_RigidJointDifferentialKinematics_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root\RigidJointDifferentialKinematics_mex.mexw64 : link $builddir/RigidJointDifferentialKinematics_data.o $builddir/rt_nonfinite.o $builddir/RigidJointDifferentialKinematics_initialize.o $builddir/RigidJointDifferentialKinematics_terminate.o $builddir/RigidJointDifferentialKinematics.o $builddir/eml_mtimes_helper.o $builddir/mtimes.o $builddir/norm.o $builddir/eye.o $builddir/dinamico_adj.o $builddir/_coder_RigidJointDifferentialKinematics_api.o $builddir/_coder_RigidJointDifferentialKinematics_mex.o $builddir/_coder_RigidJointDifferentialKinematics_info.o $builddir/c_mexapi_version.o 
