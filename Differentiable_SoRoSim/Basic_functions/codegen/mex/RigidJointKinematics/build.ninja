# CompilerName=mingw64
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/win64
matlabdir = C:/Program Files/MATLAB/R2024a
startdir = D:/Research/Soft Robotics Toolbox/SoRoSim GitHub/SoRoSim/Differentiable_SoRoSim/Basic_functions

# Toolchain information
cc = C:\ProgramData\MATLAB\SupportPackages\R2024a\3P.instrset\mingw_w64.instrset\bin\gcc
cxx = C:\ProgramData\MATLAB\SupportPackages\R2024a\3P.instrset\mingw_w64.instrset\bin\g++
linker = C:\ProgramData\MATLAB\SupportPackages\R2024a\3P.instrset\mingw_w64.instrset\bin\gcc
linkerxx = C:\ProgramData\MATLAB\SupportPackages\R2024a\3P.instrset\mingw_w64.instrset\bin\g++

cflags =  -c -fexceptions -fno-omit-frame-pointer -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c99 -I "D:\Research\Soft Robotics Toolbox\SoRoSim GitHub\SoRoSim\Differentiable_SoRoSim\Basic_functions\codegen\mex\RigidJointKinematics" -I "D:\Research\Soft Robotics Toolbox\SoRoSim GitHub\SoRoSim\Differentiable_SoRoSim\Basic_functions" -I "D:\Research\Soft Robotics Toolbox\SoRoSim GitHub\SoRoSim\Differentiable_SoRoSim\Basic_functions\codegen\mex\RigidJointKinematics\interface" -I "C:\Program Files\MATLAB\R2024a\extern\include" -I "." -DMODEL=RigidJointKinematics_mex -D__USE_MINGW_ANSI_STDIO=1 -msse2 -fno-predictive-commoning 

cxxflags =  -c -fexceptions -fno-omit-frame-pointer -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c++11 -I "D:\Research\Soft Robotics Toolbox\SoRoSim GitHub\SoRoSim\Differentiable_SoRoSim\Basic_functions\codegen\mex\RigidJointKinematics" -I "D:\Research\Soft Robotics Toolbox\SoRoSim GitHub\SoRoSim\Differentiable_SoRoSim\Basic_functions" -I "D:\Research\Soft Robotics Toolbox\SoRoSim GitHub\SoRoSim\Differentiable_SoRoSim\Basic_functions\codegen\mex\RigidJointKinematics\interface" -I "C:\Program Files\MATLAB\R2024a\extern\include" -I "." -DMODEL=RigidJointKinematics_mex -D__USE_MINGW_ANSI_STDIO=1 -msse2 -fno-predictive-commoning 

ldflags =  -m64 -Wl,--no-undefined -shared -static -L"C:\Program Files\MATLAB\R2024a\extern\lib\win64\mingw64" -llibmx -llibmex -llibmat -lm -llibmwlapack -llibmwblas -o "RigidJointKinematics_mex.mexw64" -L"$matlabdir\extern\lib\win64\mingw64" -llibmwblas -llibemlrt -llibcovrt -llibut -llibmwmathutil  RigidJointKinematics_mex.def

linkobjs = 

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker @$out.rsp $linkobjs $ldflags
    rspfile = $out.rsp
    rspfile_content = $in

rule linkxx
    command = $linkerxx $in $linkobjs $ldflags

# Build
build $builddir/RigidJointKinematics_data.o : cc $root/RigidJointKinematics_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/RigidJointKinematics_initialize.o : cc $root/RigidJointKinematics_initialize.c
build $builddir/RigidJointKinematics_terminate.o : cc $root/RigidJointKinematics_terminate.c
build $builddir/RigidJointKinematics.o : cc $root/RigidJointKinematics.c
build $builddir/_coder_RigidJointKinematics_api.o : cc $root/interface/_coder_RigidJointKinematics_api.c
build $builddir/_coder_RigidJointKinematics_mex.o : cc $root/interface/_coder_RigidJointKinematics_mex.c
build $builddir/_coder_RigidJointKinematics_info.o : cc $root/interface/_coder_RigidJointKinematics_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root\RigidJointKinematics_mex.mexw64 : link $builddir/RigidJointKinematics_data.o $builddir/rt_nonfinite.o $builddir/RigidJointKinematics_initialize.o $builddir/RigidJointKinematics_terminate.o $builddir/RigidJointKinematics.o $builddir/_coder_RigidJointKinematics_api.o $builddir/_coder_RigidJointKinematics_mex.o $builddir/_coder_RigidJointKinematics_info.o $builddir/c_mexapi_version.o 
